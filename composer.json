{
    "name": "matrozov/yii2-nested-field-validation",
    "description": "Nested field validation for Yii2",
    "keywords": [
        "yii2",
        "yii2-extension",
        "yii2 validator",
        "yii2 nested field validation"
    ],
    "homepage": "https://github.com/matrozov/yii2-nested-field-validation",
    "type": "extension",
    "version": "1.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Matrozov",
            "email": "oleg.matrozov@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/matrozov/yii2-nested-field-validation/issues",
        "source": "https://github.com/matrozov/yii2-nested-field-validation",
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0"
    },
    "require-dev": {
        "yiisoft/yii2": "~2.0.45"
    },
    "autoload": {
        "psr-4": {
            "matrozov\\yii2nestedFieldValidation\\": "src"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
